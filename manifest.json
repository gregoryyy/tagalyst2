{
    "name": "Tagalyst 2",
    "description": "ChatGPT content management and search, branch ruler nav",
    "version": "0.7.0",
    "manifest_version": 3,
    "minimum_chrome_version": "114",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*",
                "https://chatgpt.com/*"
            ],
            "js": [
                "content/shared/config.js",
                "content/shared/storage.js",
                "content/content/constants.js",
                "content/content/utils.js",
                "content/content/services/storage.js",
                "content/content/services/render-scheduler.js",
                "content/content/services/transcript.js",
                "content/content/services/dom-watcher.js",
                "content/content/adapters/registry.js",
                "content/content/adapters/api-shim.js",
                "content/content/adapters/fakes.js",
                "content/content/services/thread-renderer.js",
                "content/content/services/thread-metadata.js",
                "content/shared/globals.js",
                "content/content/services/page-classifier.js",
                "content/content/services/config.js",
                "content/content/state/message-meta.js",
                "content/content/state/focus.js",
                "content/content/controllers/keyboard.js",
                "content/content/controllers/top-panel.js",
                "content/content/controllers/overview-ruler.js",
                "content/content/controllers/editor.js",
                "content/content/controllers/highlight.js",
                "content/content/controllers/thread-metadata.js",
                "content/content/controllers/sidebar-labels.js",
                "content/content/controllers/project-list-labels.js",
                "content/content/controllers/thread-actions.js",
                "content/content/controllers/toolbar.js",
                "content/content/controllers/export.js",
                "content/content/dom/message-adapters.js",
                "content/content/dom/thread-dom.js",
                "content/content/dom/chatgpt-adapter.js",
                "content/content/markdown.js",
                "content/content/globals.js",
                "content/content.js"
            ],
            "css": [
                "content/content.css"
            ],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "ChatGPT DOM Tools"
    },
    "options_page": "options/options.html"
    ,"web_accessible_resources": [
        {
            "resources": [
                "content/keymap.yaml"
            ],
            "matches": [
                "https://chat.openai.com/*",
                "https://chatgpt.com/*"
            ]
        }
    ]
}
